   <reference bind="bindEventAdmin" interface="org.osgi.service.event.EventAdmin" name="EventAdmin" unbind="unbindEventAdmin"/>
   <reference bind="bindLicenseRegistry" interface="org.eclipse.passage.loc.internal.licenses.LicenseRegistry" name="LicenseRegistry" unbind="unbindLicenseRegistry"/>
   <reference bind="bindProductOperatorService" interface="org.eclipse.passage.loc.api.OperatorProductService" name="ProductOperatorService" unbind="unbindProductOperatorService"/>
   <reference bind="bindProductRegistry" interface="org.eclipse.passage.lic.products.registry.ProductRegistry" name="ProductRegistry" unbind="unbindProductRegistry"/>
   <reference bind="bindStreamCodec" cardinality="0..1" interface="org.eclipse.passage.lic.api.io.StreamCodec" name="StreamCodec" unbind="unbindStreamCodec"/>
   <reference bind="bindUserRegistry" interface="org.eclipse.passage.lic.users.registry.UserRegistry" name="UserRegistry" unbind="unbindUserRegistry"/>
