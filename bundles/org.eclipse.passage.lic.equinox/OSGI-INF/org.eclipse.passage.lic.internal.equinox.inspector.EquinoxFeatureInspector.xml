<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" name="org.eclipse.passage.lic.internal.equinox.inspector.EquinoxFeatureInspector">
   <property name="event.topics" value="org/eclipse/passage/lic/events/AccessManagerEvents/*"/>
   <service>
      <provide interface="org.eclipse.passage.lic.runtime.inspector.FeatureInspector"/>
      <provide interface="org.osgi.service.event.EventHandler"/>
   </service>
   <reference bind="bindAccessManager" interface="org.eclipse.passage.lic.runtime.access.AccessManager" name="AccessManager" unbind="unbindAccessManager"/>
   <implementation class="org.eclipse.passage.lic.internal.equinox.inspector.EquinoxFeatureInspector"/>
</scr:component>